<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="TalTech ÕIS v0.1 :)" Height="700" Width="1160">
    <Grid>
        <StackPanel Name="pnlInitialView" Visibility="Visible">
            <TabControl>
                <TabItem Header="Õppekavad">
                    <Grid Margin="10" Height="600">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition/>
                            <RowDefinition Height="50"/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0" Text="Õppekavade loetelu" FontSize="18" FontWeight="Bold"/>
                        <ListView Grid.Row="1" ItemsSource="{Binding Curriculums}" Name="lwCurriculums" MouseDoubleClick="lwCurriculumsDoubleClick">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="Õppekava nimi" DisplayMemberBinding="{Binding EtName}" Width="250"/>
                                    <GridViewColumn Header="Õppekava nimi (EN)" DisplayMemberBinding="{Binding EnName}" Width="250"/>
                                    <GridViewColumn Header="Õppekava kood" DisplayMemberBinding="{Binding Code}" Width="90"/>
                                    <GridViewColumn Header="Programmijuht" DisplayMemberBinding="{Binding ManagerName}" Width="180"/>
                                    <GridViewColumn Header="Õppetase" DisplayMemberBinding="{Binding StudyLevelText}" Width="110"/>
                                    <GridViewColumn Header="Õppekava maht (EAP)" DisplayMemberBinding="{Binding EapVolume}" Width="140"/>
                                    <GridViewColumn Header="Õppetöö keel" DisplayMemberBinding="{Binding LanguageText}" Width="90"/>
                                </GridView>
                            </ListView.View>
                        </ListView>
                        <Button Grid.Row="2" Width="150" Height="30" HorizontalAlignment="Right" Name="btnÓpenAddCurriculumView" Content="Lisa õppekava" Click="btnÓpenAddCurriculumView_Click"/>
                    </Grid>
                </TabItem>
                <TabItem Header="Õppeained">
                    <Grid Margin="10" Height="600">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition/>
                            <RowDefinition Height="50"/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0" Text="Õppeainete loetelu" FontSize="18" FontWeight="Bold"/>
                        <ListView Grid.Row="1" ItemsSource="{Binding Subjects}" Name="lwSubjects" MouseDoubleClick="lwSubjectsDoubleClick">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="Õppeaine nimi" DisplayMemberBinding="{Binding EtName}" Width="250"/>
                                    <GridViewColumn Header="Õppeaine nimi (EN)" DisplayMemberBinding="{Binding EnName}" Width="250" />
                                    <GridViewColumn Header="Õppeaine kood" DisplayMemberBinding="{Binding Code}" Width="90"/>
                                    <GridViewColumn Header="Õppejõud" DisplayMemberBinding="{Binding TeacherName}" Width="250"/>
                                    <GridViewColumn Header="Õppeaine maht (EAP)" DisplayMemberBinding="{Binding EapVolume}" Width="140" />
                                    <GridViewColumn Header="Hindamisvorm" DisplayMemberBinding="{Binding AssessmentFormText}" Width="140" />
                                </GridView>
                            </ListView.View>
                        </ListView>
                        <Button Grid.Row="2" Width="150" Height="30" HorizontalAlignment="Right" Name="btnOpenAddSubjectView" Content="Lisa õppeaine" Click="btnOpenAddSubjectView_Click"/>
                    </Grid>
                </TabItem>
            </TabControl>
        </StackPanel>
        <StackPanel Name="pnlEditCurriculumView" Visibility="Hidden">
            <Grid Margin="10" Height="600">
                <Grid.RowDefinitions>
                    <RowDefinition Height="80"/>
                    <RowDefinition/>
                    <RowDefinition Height="60"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="100"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Text="Õppekava muutmine" FontSize="18" FontWeight="Bold"/>
                <Grid Grid.Row="1" Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="40"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Margin="0,25,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="300"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" Grid.Column="0" Margin="0,0,0,15" Content="Õppekava nimi:" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="0" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.EtName}" />
                        <Label Grid.Row="1" Grid.Column="0" Margin="0,0,0,15" Content="Õppekava nimi (EN):" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="1" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.EnName}" />
                        <Label Grid.Row="2" Grid.Column="0" Margin="0,0,0,15" Content="Õppekava kood:" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="2" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.Code}" />
                        <Label Grid.Row="3" Grid.Column="0" Margin="0,0,0,15" Content="Programmijuht:" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="3" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.ManagerName}" />
                        <Label Grid.Row="4" Grid.Column="0" Margin="0,0,0,15" Content="Õppetase:" VerticalAlignment="Center"/>
                        <ComboBox Grid.Row="4" Grid.Column="1" Margin="0,0,0,15" ItemsSource="{Binding StudyLevels}" SelectedValuePath="Key" SelectedValue="{Binding CurrentCurriculum.StudyLevel, Mode=TwoWay}">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Value}" />
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                        <Label Grid.Row="5" Grid.Column="0" Margin="0,0,0,15" Content="Õppekava maht (EAP):" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="5" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.EapVolume}" />
                        <Label Grid.Row="6" Grid.Column="0" Margin="0,0,0,15" Content="Õppetöö keel:" VerticalAlignment="Center"/>
                        <ComboBox Grid.Row="6" Grid.Column="1" Margin="0,0,0,15" ItemsSource="{Binding Languages}" SelectedValuePath="Key" SelectedValue="{Binding CurrentCurriculum.Language, Mode=TwoWay}">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Value}" />
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                    </Grid>
                    <Grid Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="175"/>
                            <RowDefinition Height="50"/>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="175"/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0" Text="Seotud õppeained" FontSize="14" FontWeight="Bold"/>
                        <ListView Grid.Row="1" ItemsSource="{Binding CurrentCurriculumSubjects}" Name="lwCurrentCurriculumSubjects">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="Õppeaine nimi" DisplayMemberBinding="{Binding EtName}" Width="280"/>
                                    <GridViewColumn Header="Õppeaine kood" DisplayMemberBinding="{Binding Code}" Width="90"/>
                                </GridView>
                            </ListView.View>
                        </ListView>
                        <Grid Grid.Row="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="180"/>
                                <ColumnDefinition Width="180"/>
                            </Grid.ColumnDefinitions>
                            <Button Grid.Column="0" Height="30" Width="50" Click="btnRemoveSubjectFromCurriculum_Click" Content="↓" FontSize="18" FontWeight="Bold"/>
                            <Button Grid.Column="1" Height="30" Width="50" Click="btnAddSubjectToCurriculum_Click" Content="↑" FontSize="18" FontWeight="Bold"/>
                        </Grid>
                        <TextBlock Grid.Row="3" Text="Saadaval õppeained" FontSize="14" FontWeight="Bold"/>
                        <ListView Grid.Row="4" ItemsSource="{Binding CurrentCurriculumAvailableSubjects}" Name="lwAvailableSubjects">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="Õppeaine nimi" DisplayMemberBinding="{Binding EtName}" Width="280"/>
                                    <GridViewColumn Header="Õppeaine kood" DisplayMemberBinding="{Binding Code}" Width="90"/>
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </Grid>
                </Grid>
                <Button Grid.Row="2" Grid.Column="1" Height="30" HorizontalAlignment="Left"
                        Click="lnkGoHome_Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"
                        Background="Transparent" BorderThickness="0">
                    <Label>
                        <Underline>Tagasi avalehele</Underline>
                    </Label>
                </Button>
                <Grid Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Button Grid.Row="0" Height="30"
                            Click="btnDeleteCurriculum_Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"
                            Background="Transparent" BorderThickness="0">
                        <Label>
                            <Underline>Kustuta õppekava</Underline>
                        </Label>
                    </Button>
                    <Button Grid.Row="1" Height="30" Width="130" Click="btnEditCurriculum_Click" Content="Salvesta muudatused"/>
                </Grid>
            </Grid>
        </StackPanel>
        <StackPanel Name="pnlAddCurriculumView" Visibility="Hidden">
            <Grid Margin="10" Height="600">
                <Grid.RowDefinitions>
                    <RowDefinition Height="80"/>
                    <RowDefinition/>
                    <RowDefinition Height="60"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="250"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="250"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Text="Õppekava lisamine" FontSize="18" FontWeight="Bold"/>
                <Grid Grid.Row="1" Grid.Column="1" Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="300"/>
                        <ColumnDefinition Width="300"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Row="0" Grid.Column="1" Margin="0,0,0,15" Content="Õppekava nimi:" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="0" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.EtName}" />
                    <Label Grid.Row="1" Grid.Column="1" Margin="0,0,0,15" Content="Õppekava nimi (EN):" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="1" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.EnName}" />
                    <Label Grid.Row="2" Grid.Column="1" Margin="0,0,0,15" Content="Õppekava kood:" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="2" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.Code}" />
                    <Label Grid.Row="3" Grid.Column="1" Margin="0,0,0,15" Content="Programmijuht:" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="3" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.ManagerName}" />
                    <Label Grid.Row="4" Grid.Column="1" Margin="0,0,0,15" Content="Õppetase:" VerticalAlignment="Center"/>
                    <ComboBox Grid.Row="4" Grid.Column="2" Margin="0,0,0,15" ItemsSource="{Binding StudyLevels}" SelectedValuePath="Key" SelectedValue="{Binding CurrentCurriculum.StudyLevel, Mode=TwoWay}">
                        <ComboBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Value}" />
                            </DataTemplate>
                        </ComboBox.ItemTemplate>
                    </ComboBox>
                    <Label Grid.Row="5" Grid.Column="1" Margin="0,0,0,15" Content="Õppekava maht (EAP):" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="5" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentCurriculum.EapVolume}" />
                    <Label Grid.Row="6" Grid.Column="1" Margin="0,0,0,15" Content="Õppetöö keel:" VerticalAlignment="Center"/>
                    <ComboBox Grid.Row="6" Grid.Column="2" Margin="0,0,0,15" ItemsSource="{Binding Languages}" SelectedValuePath="Key" SelectedValue="{Binding CurrentCurriculum.Language, Mode=TwoWay}">
                        <ComboBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Value}"/>
                            </DataTemplate>
                        </ComboBox.ItemTemplate>
                    </ComboBox>
                </Grid>
                <Button Grid.Row="2" Grid.Column="1" Height="30" HorizontalAlignment="Left"
                Click="lnkGoHome_Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"
                Background="Transparent" BorderThickness="0">
                    <Label>
                        <Underline>Tagasi avalehele</Underline>
                    </Label>
                </Button>
                <Button Grid.Row="2" Grid.Column="1"  Width="150" Height="30" HorizontalAlignment="Right" Name="btnAddCurriculum" Click="btnAddCurriculum_Click" Content="Lisa õppekava"/>
            </Grid>
        </StackPanel>
        <StackPanel Name="pnlEditSubjectView" Visibility="Hidden">
            <Grid Margin="10" Height="600">
                <Grid.RowDefinitions>
                    <RowDefinition Height="80"/>
                    <RowDefinition/>
                    <RowDefinition Height="60"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="100"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Text="Õppeaine muutmine" FontSize="18" FontWeight="Bold"/>
                <Grid Grid.Row="1" Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="40"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Margin="0,25,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="300"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" Grid.Column="0" Margin="0,0,0,15" Content="Õppeaine nimi:" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="0" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentSubject.EtName}" />
                        <Label Grid.Row="1" Grid.Column="0" Margin="0,0,0,15" Content="Õppeaine nimi (EN):" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="1" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentSubject.EnName}" />
                        <Label Grid.Row="2" Grid.Column="0" Margin="0,0,0,15" Content="Õppeaine kood:" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="2" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentSubject.Code}" />
                        <Label Grid.Row="3" Grid.Column="0" Margin="0,0,0,15" Content="Õppejõud:" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="3" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentSubject.TeacherName}" />
                        <Label Grid.Row="4" Grid.Column="0" Margin="0,0,0,15" Content="Õppeaine maht (EAP):" VerticalAlignment="Center"/>
                        <TextBox Grid.Row="4" Grid.Column="1" Margin="0,0,0,15" Text="{Binding CurrentSubject.EapVolume}" />
                        <Label Grid.Row="5" Grid.Column="0" Margin="0,0,0,15" Content="Hindamisvorm:" VerticalAlignment="Center"/>
                        <ComboBox Grid.Row="5" Grid.Column="1" Margin="0,0,0,15" ItemsSource="{Binding AssessmentForms}" SelectedValuePath="Key" SelectedValue="{Binding CurrentSubject.AssessmentForm, Mode=TwoWay}">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Value}" />
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                    </Grid>
                    <Grid Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="250"/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Row="0" Text="Seotud õppekavad" FontSize="14" FontWeight="Bold"/>
                        <ListView Grid.Row="1" ItemsSource="{Binding CurrentSubjectCurriculums}" Name="lwCurrentSubjectCurriculums">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="Õppekava nimi" DisplayMemberBinding="{Binding EtName}" Width="280"/>
                                    <GridViewColumn Header="Õppekava kood" DisplayMemberBinding="{Binding Code}" Width="90"/>
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </Grid>
                </Grid>
                <Button Grid.Row="2" Grid.Column="1" Height="30" HorizontalAlignment="Left"
            Click="lnkGoHome_Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"
            Background="Transparent" BorderThickness="0">
                    <Label>
                        <Underline>Tagasi avalehele</Underline>
                    </Label>
                </Button>
                <Grid Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Button Grid.Row="0" Height="30"
                Click="btnDeleteCurriculum_Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"
                Background="Transparent" BorderThickness="0">
                        <Label>
                            <Underline>Kustuta õppeaine</Underline>
                        </Label>
                    </Button>
                    <Button Grid.Row="1" Height="30" Width="130" Click="btnEditCurriculum_Click" Content="Salvesta muudatused"/>
                </Grid>
            </Grid>
        </StackPanel>
        <StackPanel Name="pnlAddSubjectView" Visibility="Hidden">
            <Grid Margin="10" Height="600">
                <Grid.RowDefinitions>
                    <RowDefinition Height="80"/>
                    <RowDefinition/>
                    <RowDefinition Height="60"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="250"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="250"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Text="Õppeaine lisamine" FontSize="18" FontWeight="Bold"/>
                <Grid Grid.Row="1" Grid.Column="1" Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="300"/>
                        <ColumnDefinition Width="300"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Row="0" Grid.Column="1" Margin="0,0,0,15" Content="Õppeaine nimi:" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="0" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentSubject.EtName}" />
                    <Label Grid.Row="1" Grid.Column="1" Margin="0,0,0,15" Content="Õppeaine nimi (EN):" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="1" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentSubject.EnName}" />
                    <Label Grid.Row="2" Grid.Column="1" Margin="0,0,0,15" Content="Õppeaine kood:" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="2" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentSubject.Code}" />
                    <Label Grid.Row="3" Grid.Column="1" Margin="0,0,0,15" Content="Õppejõud:" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="3" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentSubject.TeacherName}" />
                    <Label Grid.Row="4" Grid.Column="1" Margin="0,0,0,15" Content="Õppeaine maht (EAP):" VerticalAlignment="Center"/>
                    <TextBox Grid.Row="4" Grid.Column="2" Margin="0,0,0,15" Text="{Binding CurrentSubject.EapVolume}" />
                    <Label Grid.Row="5" Grid.Column="1" Margin="0,0,0,15" Content="Hindamisvorm:" VerticalAlignment="Center"/>
                    <ComboBox Grid.Row="5" Grid.Column="2" Margin="0,0,0,15" ItemsSource="{Binding AssessmentForms}" SelectedValuePath="Key" SelectedValue="{Binding CurrentSubject.AssessmentForm, Mode=TwoWay}">
                        <ComboBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Value}" />
                            </DataTemplate>
                        </ComboBox.ItemTemplate>
                    </ComboBox>
                </Grid>
                <Button Grid.Row="2" Grid.Column="1" Height="30" HorizontalAlignment="Left"
                        Click="lnkGoHome_Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"
                        Background="Transparent" BorderThickness="0">
                    <Label>
                        <Underline>Tagasi avalehele</Underline>
                    </Label>
                </Button>
                <Button Grid.Row="2" Grid.Column="1"  Width="150" Height="30" HorizontalAlignment="Right" Name="btnAddSubject" Click="btnAddSubject_Click" Content="Lisa õppeaine"/>
            </Grid>
        </StackPanel>
        <ComboBox HorizontalAlignment="Left" Margin="1240,105,0,0" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>